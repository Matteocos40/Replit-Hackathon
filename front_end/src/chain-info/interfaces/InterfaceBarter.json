  "source": "// SPDX-License-Identifier: MIT\n\npragma solidity ^0.8.0;\n\ninterface InterfaceBarter {\n    function exchangeNFT(\n        address _buyer,\n        address _seller,\n        address _collection,\n        uint256 _tokenID\n    ) external;\n\n    function collateralizedPurchase(\n        address _buyer,\n        address _seller,\n        address _collection,\n        uint256 _tokenID,\n        uint256 _itemValue\n    ) external;\n\n    function repay(\n        address _collection,\n        uint256 _tokenID,\n        uint256 _amount\n    ) external;\n\n    function totalValueBorrowed(address _borrower)\n        external\n        view\n        returns (uint256);\n\n    function valueBorrowedOneNFT(\n        address _buyer,\n        address _collection,\n        uint256 _tokenID\n    ) external view returns (uint256);\n}\n",
  "type": "interface"
}